---
---
[
  {% for post in site.posts %}
    {
      "title"       : "{{ post.title }}",
      "tags"        : ["{{ post.tags | join: '", "' | }}"],
      "url"         : "{{ post.url }}",
      "description" : "{{ post.description }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
